<?xml version="1.0" encoding="utf-8"?>
<!-- Web.config transformation for UAT environment -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="Environment" value="UAT" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<!-- UAT-specific GCP project if different -->
		<add key="GcpProjectId" value="prj-tar-bks-uat-01" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
	</appSettings>
	<system.web>
		<compilation xdt:Transform="RemoveAttributes(debug)" />
	</system.web>
</configuration>

